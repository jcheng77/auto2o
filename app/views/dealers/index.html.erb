<h1>Listing dealers</h1>

<table>
  <thead>
  <tr>
    <th>id</th>
    <th>email</th>
    <th>created_at</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @dealers.each do |dealer| %>
      <tr>
        <td><%= dealer.id %></td>
        <td><%= dealer.email %></td>
        <td><%= dealer.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tender', new_dealer_path %>
